//Create Rectangle class
class Rectangle{  
 int length;  
 int width;  
//create a method insert
 void insert(int l, int w){  
  length=l;  
  width=w;  
 }  
//create a method calculate area
 void calculateArea(){System.out.println(length*width);}  
}  
//Main class that is Testrectangl1
//Import a package
import java.util.*;
class TestRectangle1{  
 public static void main(String args[]){  
//Create an object r1 and r2 in rectangle class.
  Rectangle r1=new Rectangle();  
  Rectangle r2=new Rectangle();  
//call that method by passing the parameters.
  r1.insert(11,5);  
  r2.insert(3,15);  
  r1.calculateArea();  
  r2.calculateArea();  
}  
}

//Thank you!
